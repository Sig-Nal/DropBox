  <!-- Compression -->
  <animation>
    <type>rotate</type>
    <object-name>ToutAvantGauche</object-name>
    <property>gear/gear[0]/compression-norm</property>
    <interpolation>
      <entry><ind> 0 </ind><dep>  0 </dep></entry>
      <entry><ind> 1 </ind><dep> 20 </dep></entry>
    </interpolation>
    <center>
      <x-m> -0.694 </x-m>
      <y-m> -0.444 </y-m>
      <z-m> -0.379 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

